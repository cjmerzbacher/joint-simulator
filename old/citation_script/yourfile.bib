	issn = {1096-7176},
	journal = {Metabolic Engineering},
	keywords = {Bioprocess modeling, Dynamic flux balance analysis, Machine learning, Metabolic models},
	month = mar,
	pages = {183--192},
	shorttitle = {{COSMIC}-{dFBA}},
	title = {{COSMIC}-{dFBA}: {A} novel multi-scale hybrid framework for bioprocess modeling},
	url = {https://www.sciencedirect.com/science/article/pii/S1096717624000284},
	urldate = {2024-03-28},
