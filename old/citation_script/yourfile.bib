	issn = {10958541},
	journal = {Journal of Theoretical Biology},
	keywords = {Bacterial growth, Constraint-based methods, Flux optimization, Metabolic-genetic networks},
	month = nov,
	note = {arXiv: 1309.4936},
	pages = {469--485},
	title = {Dynamic optimization of metabolic networks coupled with gene expression},
